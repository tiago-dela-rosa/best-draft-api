{
   "name": "best-draft-api",
   "version": "1.0.0",
   "description": "Tool to draft-pick",
   "main": "index.js",
   "scripts": {
      "start": "set NODE_ENV=development&& --exec babel-node src/server",
      "start:dev": "set NODE_ENV=development&& nodemon --exec babel-node src/server",
      "lint": "eslint src",
      "lint:fix": "eslint --fix src",
      "test": "echo \"Error: no test specified\" && exit 1",
      "migrate": "knex migrate:latest --knexfile ./src/db/config.js",
      "migrate:rollback": "knex migrate:rollback --knexfile ./src/db/config.js",
      "seed": "knex seed:run --knexfile ./src/db/config.js"
   },
   "author": "Tiago Dela Rosa Lopes",
   "license": "ISC",
