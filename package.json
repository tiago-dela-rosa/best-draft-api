{
   "name": "best-draft-api",
   "version": "1.0.0",
   "description": "Tool to draft-pick",
   "main": "index.js",
   "scripts": {
      "start": "ts-node src/index.ts",
      "start:dev": "set NODE_ENV=development&& nodemon --exec babel-node src/app",
      "lint": "eslint src",
      "lint:fix": "eslint --fix src",
      "test": "echo \"Error: no test specified\" && exit 1",
      "migrate": "knex migrate:latest --knexfile ./src/db/config.js",
      "migrate:rollback": "knex migrate:rollback --knexfile ./src/db/config.js",
      "seed": "knex seed:run --knexfile ./src/db/config.js"
   },
   "author": "Tiago Dela Rosa Lopes",
   "license": "ISC",
   "dependencies": {
      "babel-eslint": "^10.1.0",
      "body-parser": "^1.19.0",
      "dotenv": "^8.2.0",
      "eslint-config-airbnb": "^18.1.0",
      "eslint-plugin-import": "^2.20.1",
      "express": "^4.17.1",
      "faker": "^4.1.0",
      "knex": "^0.20.11",
      "nodemon": "^2.0.2",
      "pg": "^7.18.2",
      "reflect-metadata": "^0.1.10",
      "typeorm": "0.2.24",
      "winston": "^3.2.1",
      "mysql": "^2.14.1"
   },
   "devDependencies": {
      "@babel/core": "^7.8.7",
      "@babel/node": "^7.8.7",
      "@babel/plugin-proposal-class-properties": "^7.8.3",
      "@babel/preset-env": "^7.8.7",
      "eslint": "^6.8.0",
      "eslint-plugin-jsx-a11y": "^6.2.3",
      "ts-node": "3.3.0",
      "@types/node": "^8.0.29",
      "typescript": "3.3.3333"
   }
}