      "celebrate": "^12.2.0",
      "dayjs": "^1.9.4",
      "dotenv": "^8.2.0",
      "eslint-config-airbnb": "^18.2.0",
      "eslint-plugin-import": "^2.22.0",
      "express": "^4.17.1",
      "faker": "^4.1.0",
      "jsonwebtoken": "^8.5.1",
      "knex": "^0.20.15",
      "lodash": "^4.17.20",
      "md5": "^2.3.0",
      "nodemon": "^2.0.4",
